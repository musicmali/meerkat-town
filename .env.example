# Meerkat Town Frontend Environment Variables
# Copy this file to .env and fill in your values

# Backend API URL
VITE_BACKEND_URL=http://localhost:3000

# Alchemy API Key (optional - free public RPCs used when not provided)
# Base Sepolia always uses free RPC, Mainnet uses Alchemy if key is provided
VITE_ALCHEMY_API_KEY=

# Pinata JWT for IPFS uploads (required for minting agents)
VITE_PINATA_JWT=
